<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.team_htbr.a1617proj1bloeddonatie_app.MainActivity">

	<Button
		android:id="@+id/Bloodtype"
		android:layout_width="0dp"
		android:layout_height="51dp"
		android:layout_marginEnd="23dp"
		android:layout_marginLeft="23dp"
		android:layout_marginRight="23dp"
		android:layout_marginStart="23dp"
		android:layout_marginTop="16dp"
		android:text="Selecteer mijn bloedtype"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_constraintLeft_creator="1"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintTop_creator="1" />

	<Button
		android:id="@+id/donorTest"
		android:layout_width="0dp"
		android:layout_height="52dp"
		android:layout_marginEnd="22dp"
		android:layout_marginLeft="22dp"
		android:layout_marginRight="22dp"
		android:layout_marginStart="22dp"
		android:text="Zelfdonortest"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/Bloodtype"
		tools:layout_constraintLeft_creator="1"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintTop_creator="1" />

	<Button
		android:id="@+id/GoogleMap"
		android:layout_width="0dp"
		android:layout_height="48dp"
		android:layout_marginEnd="20dp"
		android:layout_marginLeft="20dp"
		android:layout_marginRight="20dp"
		android:layout_marginStart="20dp"
		android:layout_marginTop="-292dp"
		android:text="Donorcentra"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/donorTest"
		tools:layout_constraintLeft_creator="1"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintTop_creator="1" />

</android.support.constraint.ConstraintLayout>
